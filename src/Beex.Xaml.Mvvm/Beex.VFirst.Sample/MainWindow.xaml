<Window x:Class="Beex.VFirst.Sample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Beex.VFirst.Sample"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow"
        Width="650"
        Height="520"
        DataContext="{Binding Source={StaticResource Locator}, Path=MianVm}"
        WindowStartupLocation="CenterScreen"
        WindowState="Normal"
        mc:Ignorable="d">
    <Grid>
        <ContentControl Background="AliceBlue" Content="{Binding Path=CurrentView, Mode=OneWay}" />
    </Grid>
</Window>
